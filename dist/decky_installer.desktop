#!/usr/bin/env xdg-open
[Desktop Entry]
Name=Install Decky
Exec=sh -c 'curl -O --output-dir /tmp/ https://raw.githubusercontent.com/PartyWumpus/decky-loader/install-script/dist/user_install_script.sh && bash /tmp/user_install_script.sh'
